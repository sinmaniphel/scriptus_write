<a class="list-group-item sw_dated_scene_item sw_scene_item"
  data-scid="{{ id }}"
  data-title="{{ scene_title }}"
  {{#if scene.start }}
  data-start="{{ start }}"
  data-end="{{ end }}"
  {{/if}}
  data-url="{{ url }}"
  >
      
  <h4 class="list-group-item-heading">{{ scene_title}}</h4>
  <p class="list-group-item-text">
      {{#if start }}
      Start : {{ start }} - End : {{ end }}
      {{else}}
      Not dated yet
      {{/if}}
      
  </p>
</a>
